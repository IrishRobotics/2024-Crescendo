package frc.robot.commands.Drivetrain;
import frc.robot.subsystems.Drivetrain;
import edu.wpi.first.wpilibj.Timer;
import edu.wpi.first.wpilibj2.command.Command;
import edu.wpi.first.wpilibj2.command.InstantCommand;

public class DumbMove extends InstantCommand {
  private Double speed;
  private Drivetrain sDrive;
  /** Creates a new DumbMove. */
  public DumbMove(Drivetrain sDrive, Double speed) {
    this.speed = speed;
    this.sDrive = sDrive;
    // Use addRequirements() here to declare subsystem dependencies.
    addRequirements(sDrive);
  }

  // Called when the command is initially scheduled.
  @Override
  public void initialize() {
    sDrive.Drive(speed,0,0,false);
    Timer.delay(5);
    sDrive.Drive(0, 0, 0,false);
  }

}
